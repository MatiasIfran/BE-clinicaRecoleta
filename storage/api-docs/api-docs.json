{
    "openapi": "3.0.0",
    "info": {
        "title": "Api's Clinica Recoleta",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000"
        }
    ],
    "paths": {
        "/api/cp/all": {
            "get": {
                "tags": [
                    "Codigo Postal"
                ],
                "summary": "Obtener todos los códigos postales",
                "description": "Retorna una lista de todos los códigos postales disponibles",
                "operationId": "getAllCodigosPostales",
                "responses": {
                    "200": {
                        "description": "Operación exitosa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean"
                                        },
                                        "codigos_postales": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "number"
                                                    },
                                                    "codigo": {
                                                        "type": "string"
                                                    },
                                                    "descripcion": {
                                                        "type": "string"
                                                    },
                                                    "usuario": {
                                                        "type": "string"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "apiAuth": []
                    }
                ]
            }
        },
        "/api/cp/id/{codigoPostalId}": {
            "get": {
                "tags": [
                    "Codigo Postal"
                ],
                "summary": "Obtener codigo postal por Id",
                "description": "Retorna un array del codigo postal disponible",
                "operationId": "getCodPostById",
                "parameters": [
                    {
                        "name": "codigoPostalId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operación exitosa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean"
                                        },
                                        "codigos_postales": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "number"
                                                    },
                                                    "codigo": {
                                                        "type": "string"
                                                    },
                                                    "descripcion": {
                                                        "type": "string"
                                                    },
                                                    "usuario": {
                                                        "type": "string"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "apiAuth": []
                    }
                ]
            }
        },
        "/api/cp/{codigoPostalNumber}": {
            "get": {
                "tags": [
                    "Codigo Postal"
                ],
                "summary": "Obtener codigo postal por numero",
                "description": "Retorna un array del codigo postal disponible",
                "operationId": "getCodPostByCodigo",
                "parameters": [
                    {
                        "name": "codigoPostalNumber",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operación exitosa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean"
                                        },
                                        "codigos_postales": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "number"
                                                    },
                                                    "codigo": {
                                                        "type": "string"
                                                    },
                                                    "descripcion": {
                                                        "type": "string"
                                                    },
                                                    "usuario": {
                                                        "type": "string"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "apiAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "apiAuth": {
                "type": "http",
                "description": "Login with email and password to get the authentication token",
                "name": "Token based Based",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}